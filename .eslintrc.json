{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "browser": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "error",
        "quotes": [2, "single"],
        "comma-dangle": "off"
    }
}
